#!/usr/bin/expect

set ip [lindex $argv 1]

set name [lindex $argv 0]

spawn ssh $name@$ip -l root
set timeout 2
expect {
"yes/no" {send "yes\r";exp_continue}
"Password:" {send "root\r"}
}
set timeout 2
spawn ssh $name@$ip "ls > xxoo.txt\r"
expect {
"yes/no" {send "yes\r";exp_continue}
"Password:" {send "root\r"}
}
expect eof


